block header
  div(role="banner" id="header")
    h1(class="margin--small")= bio.name
    h2(class="margin--small")= bio.title
    div(class="dropdown")
      div(class="margin--small fa fa-bars clickable selectable" id="menu-icon")
      span(class="dropdown-content")
        a(href="/about") about
        a(href="/blog") blog
        a(href="/resume") resume
